<!DOCTYPE html>
<html>
<head>
  <title>DeviseLab</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= content_tag(:div, flash[:error], :id => "flash_error") if flash[:error] %>
  <%= content_tag(:div, flash[:notice], :id => "flash_notice") if flash[:notice] %>
  <%= content_tag(:div, flash[:alert], :id => "flash_alert") if flash[:alert] %>
  <%= yield %>
  <p><%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %></p>
  <p><%= link_to("Sign up", new_user_registration_path) %></p>
  <p><%= link_to("Sign in", new_user_session_path) %></p>
  <p><%= link_to("Home", root_path) %></p>
  <p><%= link_to("About", about_path) %></p>
  <p><%= link_to("Sign out", destroy_user_session_path, :method => :delete) %></p>
</body>
</html>
