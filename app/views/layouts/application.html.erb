<!DOCTYPE html>
<html>
<head>
  <title>DeviseLab</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav>
    <div><%= link_to "Home", root_path %></div>
    <div><%= link_to "Sign up", new_user_registration_path %></div>

  </nav>

  <!-- <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p> -->

  <div><%= content_tag(:div, flash[:error], :id => "flash_error") if flash[:error] %></div>
  <div><%= content_tag(:div, flash[:notice], :id => "flash_notice") if flash[:notice] %></div>
  <div><%= content_tag(:div, flash[:alert], :id => "flash_alert") if flash[:alert] %></div>

<%= yield %>
  <div><%= link_to "Sign out", destroy_user_session_path, method: :delete %></div>
</body>
</html>
