<h2>Welcome</h2>

<% if current_user %>
  <h2><%= current_user.email %></h2>
  <%= link_to("sign out", destroy_user_session_path, :method => :delete) %>
<% else %>
  <%= link_to("go to sign in", new_user_session_path) %>
  <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
<% end %>


